#call plt.xlabel() passing ‘Date’ 
plt.xlabel('Date', fontsize = 20)
#call plt.ylabel() passing ‘Price’ 
plt.ylabel('Price', fontsize = 20)
#call plt.title() passing company 
plt.title(company)
plt.show() 
